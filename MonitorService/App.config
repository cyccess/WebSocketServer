<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="RedisConfig" type="MonitorService.Redis.RedisConfig,MonitorService"/>
  </configSections>
  <RedisConfig WriteServerConStr="127.0.0.1:6379" ReadServerConStr="127.0.0.1:6379" MaxWritePoolSize="60"
      MaxReadPoolSize="60" AutoStart="true" LocalCacheTime="180" RecordeLog="false"/>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>

  <connectionStrings>
    <add name="conn" connectionString="server=.;database=fleck;uid=sa;pwd=123"/>
  </connectionStrings>


  <appSettings>
    <add key="WebSocketServer" value="ws://0.0.0.0:8008"/>
  </appSettings>
</configuration>